<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="file://{resources}/styles/custom_game/end_screen.css" />
	</styles>

	<scripts> 
		<include src="file://{resources}/scripts/custom_game/end_screen.js" />
	</scripts>

	<Panel hittest="false" class="EndScreenRoot">
		<Panel hittest="false" class="EndScreenWindow">
			<Panel id="VictoryRow">
				<Panel id="WinningTeamLogo" />
				<Label id="EndScreenVictory" text="#custom_end_screen_victory_message" />
			</Panel>
			<Panel id="Team1" class="Team">
				<Panel id="Team1_info" class="Teaminfo">
					<Label class="TeamName" text="#Team1Name"/>
					<Label class="TeamDamageToKing" text="#DamageToKing"/>
					<Label class="TeamBreakGold" text="#BreakGold"/>
					<Label class="TeamKillCount" text="#KillCount"/>
					<Label class="TeamScore" text="#Score"/>
					<Label class="TeamGold" text="#Gold"/>
					<Label class="TeamLumber" text="#Crystal"/>
					<Label class="TeamIncome" text="#Income"/>
					<Label class="TeamArmaments" text="#DOTA_HUDShop"/>
					<Label class="TeamMVP" text=""/>
					<Label class="TeamMMR" text="#MMR"/>
				</Panel>
			</Panel>
			<Panel id="Space"/>
			<Panel id="Team2" class="Team">
				<Panel id="Team2_info" class="Teaminfo">
					<Label class="TeamName" text="#Team2Name"/>
					<Label class="TeamDamageToKing" text="#DamageToKing"/>
					<Label class="TeamBreakGold" text="#BreakGold"/>
					<Label class="TeamKillCount" text="#KillCount"/>
					<Label class="TeamScore" text="#Score"/>
					<Label class="TeamGold" text="#Gold"/>
					<Label class="TeamLumber" text="#Crystal"/>
					<Label class="TeamIncome" text="#Income"/>
					<Label class="TeamArmaments" text="#DOTA_HUDShop"/>
					<Label class="TeamMVP" text=""/>
					<Label class="TeamMMR" text="#MMR"/>
				</Panel>
			</Panel>
			<Button id="CloseButton" onactivate="Game.FinishGame();">
				<Label text="#CustomEndScreenClose" />
			</Button>
		</Panel>
	</Panel>
</root>
